{
  "name": "vigour-status-bar",
  "version": "1.0.5",
  "description": "Native status bar control",
  "main": "lib/index.js",
  "scripts": {
    "start": "gaston",
    "test": "gaston test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/status-bar.git"
  },
  "keywords": [
    "vigour",
    "wrapper",
    "native",
    "plugin",
    "status-bar"
  ],
  "bugs": {
    "url": "https://github.com/vigour-io/status-bar/issues"
  },
  "author": "Vigour",
  "contributors": [
    "Shawn Inder <shawn@vigour.io>",
    "Michiel Van Liempt <michiel@vigour.io>",
    "Alexander van de Werff <alex@vigour.io>"
  ],
  "license": "MIT",
  "homepage": "https://github.com/vigour-io/status-bar",
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "vigour-js": "^0.3.6",
    "vigour-ua": "0.0.3",
    "vigour-wrapper": "^2.1.3"
  },
  "vigour": {
    "plugin": {
      "android": {
        "className": "io.vigour.plugin.statusbar.StatusBarPlugin",
        "instantiation": "new StatusBarPlugin(this, webview)"
      }
    }
  },
  "standard": {
    "globals": [
      "alert",
      "describe",
      "it",
      "expect",
      "before",
      "after",
      "beforeEach",
      "afterEach",
      "sinon"
    ]
  },
  "devDependencies": {
    "gaston": "^0.28.5"
  }
