{
  "name": "vigour-native-statusBar",
  "version": "0.0.1",
  "author": "Shawn Inder <shawn@vigour.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/vigour-native-statusBar"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "main": "index.js",
  "scripts": {
    "test": "test/index.js"
  },
  "description": "Allows one to control the native status bar from a web app",
  "keywords": [
    "vigour",
    "native",
    "plugin",
    "statusBar"
  ],
  "bugs": {
    "url": "https://github.com/vigour-io/vigour-native-statusBar/issues"
  },
  "homepage": "https://github.com/vigour-io/vigour-native-statusBar",
  "dependencies": {
    "vigour-native": "git+ssh://git@github.com:vigour-io/vigour-native.git#master",
    "vigour-fs": "git+ssh://git@github.com:vigour-io/vigour-fs.git#master",
    "vigour-dev-tools": "git+ssh://git@github.com:vigour-io/vigour-dev-tools.git#master"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "npm": "^2.7.6",
    "npmlog": "^1.2.0"
  },
  "plugin": {
    "id": "statusbar",
    "android": {
      "className": "io.vigour.plugin.statusbar.StatusBarPlugin",
      "instantiation": "new StatusBarPlugin(this, webView)",
      "libName": "statusbar-debug.aar"
    }
  }
}